{
  "version": 1,
  "dns": [
    "host"
  ],
  "services": {
    "consul": {
      "image": "docker://consul:0.8.2",
      "command": [
        "agent",
        "-server",
        "-ui",
        "-bootstrap-expect",
        "1",
        "-log-level",
        "info",
        "-bind",
        "0.0.0.0",
        "-client",
        "0.0.0.0",
        "-dns-port",
        "53",
        "-recursor",
        "8.8.8.8"
      ],
      "environment": {
        "CONSUL_ALLOW_PRIVILEGED_PORTS": ""
      }
    }
  }
}
